// Page Header Start
.page-main-header
                .main-header-right.row.m-0
                    .main-header-left
                        .logo-wrapper
                            a(href='index.html')
                                img.img-fluid(src='../assets/images/logo/logo.png', alt='')
                    .toggle-sidebar
                      i.status_toggle.middle(data-feather='grid')#sidebar-toggle
                    //- .d-lg-none.mobile-vertical-sidebar.pull-right.toggle-nav
                    //-     i(data-feather='align-justify')
                    .left-menu-header.col
                      ul
                        li
                          form.form-inline.search-form
                            .search-bg
                              i.fa.fa-search
                            input.form-control-plaintext(placeholder='Search here.....')
                          span.d-sm-none.mobile-search.search-bg
                            i.fa.fa-search
                    .nav-right.col.pull-right.right-menu
                        ul.nav-menus
                            li.onhover-dropdown
                                .notification-box
                                  i(data-feather='bell')
                                  span.badge.badge-pill.badge-secondary 2
                                ul.notification-dropdown.onhover-show-div
                                    li
                                      p.f-w-600.font-roboto You have 3 notifications
                                    li
                                      p.mb-0
                                        i.fa.fa-circle-o.mr-3.font-primary 
                                        | Delivery processing 
                                        span.pull-right 10 min.
                                    li
                                      p.mb-0
                                        i.fa.fa-circle-o.mr-3.font-success
                                        | Order Complete
                                        span.pull-right 1 hr
                                    li
                                      p.mb-0
                                        i.fa.fa-circle-o.mr-3.font-info
                                        | Tickets Generated
                                        span.pull-right 3 hr
                                    li
                                      p.mb-0
                                        i.fa.fa-circle-o.mr-3.font-warning
                                        | Delivery Complete
                                        span.pull-right 6 hr
                            li.onhover-dropdown
                                i(data-feather='message-square')
                                ul.chat-dropdown.onhover-show-div.p-t-15.p-b-15
                                  li
                                    .media
                                      img.img-fluid.rounded-circle.mr-3(src='../assets/images/user/1.jpg', alt='')
                                      .status-circle.away
                                      .media-body
                                        span Erica Hughes
                                        p.f-12.light-font Lorem Ipsum is simply dummy...
                                      p.f-12.font-warning 58 mins ago
                                  li
                                    .media
                                      img.img-fluid.rounded-circle.mr-3(src='../assets/images/user/2.jpg', alt='')
                                      .status-circle.online
                                      .media-body
                                        span Kori Thomas
                                        p.f-12.light-font Lorem Ipsum is simply dummy...
                                      p.f-12.font-success 1 hr ago
                                  li
                                    .media
                                      img.img-fluid.rounded-circle.mr-3(src='../assets/images/user/4.jpg', alt='')
                                      .status-circle.offline
                                      .media-body
                                        span Ain Chavez
                                        p.f-12.light-font Lorem Ipsum is simply dummy...
                                      p.f-12.font-danger 32 mins ago
                                  li.text-center 
                                    a(href="#") View All     
                            li
                                a.text-dark(href='#!', onclick='javascript:toggleFullScreen()')
                                    i(data-feather='maximize')
                            li.onhover-dropdown.p-0
                                .media.profile-media
                                  img.b-r-10(src='../assets/images/dashboard/Profile.jpg', alt='')
                                  .media-body
                                    span Emay Walter
                                    p.mb-0.font-roboto Admin 
                                      i.middle.fa.fa-angle-down
                                ul.profile-dropdown.onhover-show-div
                                  li
                                    i(data-feather='user')
                                    span Account 
                                  li
                                    i(data-feather='mail')
                                    span Inbox
                                  li
                                    i(data-feather='file-text')
                                    span Taskboard
                                  li
                                    i(data-feather='settings')
                                    span Settings
                                  li
                                    i(data-feather='log-in') 
                                    span Log in
                                    
                    .d-lg-none.mobile-toggle.pull-right
                        i(data-feather='more-horizontal')
// Page Header Ends                              
